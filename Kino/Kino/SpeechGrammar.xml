<grammar version="1.0" xml:lang="en-US" root="rootRule" tag-format="semantics/1.0-literals" xmlns="http://www.w3.org/2001/06/grammar">
  <rule id="rootRule">
    <one-of>
      <item>
        <tag>WAIT</tag>
        <one-of>
          <item> wait </item>
          <item> stop </item>
        </one-of>
      </item>
      <item>
        <tag>GO</tag>
        <one-of>
          <item> go </item>
          <item> asdf </item>
          <item> anda </item>
        </one-of>
      </item>
      <item>
        <tag>HELLO</tag>
        <one-of>
          <item> hi </item>
          <item> hey </item>
          <item> ohaio </item>
        </one-of>
      </item>
      <item>
        <tag>BRUSH</tag>
        <one-of>
          <item> paintbrush </item>
          <item> brush </item>
          <item> paint </item>
        </one-of>
      </item>
    </one-of>
  </rule>
</grammar>